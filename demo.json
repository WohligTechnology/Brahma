//for postman
{
    "path": "/Applications/XAMPP/xamppfiles/htdocs",
    "projectname": "WindowsGreen",
    "models": [
        {
            "name": "user",
            "structure": [
                {
                    "name": "name",
                    "type": "text",
                    "validation": [
                        "required",
                        "minlength",
                        "min=5"
                    ]
                }, {
                    "name": "email",
                    "type": "email",
                    "validation": [
                        "required",
                        "email"
                    ]
                }, {
                    "name": "Tag",
                    "type": "uiselect",
                    "structure": ["name"]
                }, {
                    "name": "address",
                    "type": "textarea",
                    "validation": [
                        "required"
                    ]
                }, {
                    "name": "Discount Coupon",
                    "type": "select",
                    "isfromdb": false,
                    "option": [{
                        "id": 1,
                        "name": "first option"
                    }, {
                        "id": 2,
                        "name": "second option"
                    }, {
                        "id": 3,
                        "name": "third option"
                    }, {
                        "id": 4,
                        "name": "fourth option"
                    }, {
                        "id": 5,
                        "name": "fifth option"
                    }]

                }, {
                    "name": "gallery",
                    "type": "array",
                    "structure": [
                        {
                            "name": "name",
                            "type": "text",
                            "validation": [
                                "required",
                                "minlength",
                                "min=5"
                            ]
                        },
                        {
                            "name": "image",
                            "type": "image"
                        },
                        {
                            "name": "name",
                            "type": "text",
                            "validation": [
                                "required",
                                "minlength",
                                "min=5"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}

